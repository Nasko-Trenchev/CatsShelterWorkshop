
{{#each cats}}
<li>
 <img src="{{imageUrl}}" alt="Black Cat">
  <h3>{{name}}</h3>
  <p><span>Breed: </span>{{breed}}</p>
  <p><span>Description: </span>{{description}}</p>
<ul class="buttons">
 <li class="btn edit"><a href="/cats/edit/{{id}}">Change Info</a></li>
<li class="btn delete"><a href="/cat/shelter/{{id}}">New Home</a></li>  
</ul>
</li>
 {{/each}}
